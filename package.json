{
  "name": "spottr-web",
  "version": "0.0.5",
  "description": "Mobile optmized UI for spottr MVP. Targeting Google chrome  on Galaxy S6Edge",
  "options": {
    "mocha": "tools/testSetup.js src/**/*.spec.js --reporter progress"
  },
  "scripts": {
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel test:watch open:src",
    "lint": "esw webpack.config.* src tools --color --ignore-pattern node_modules; exit 0;",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist && npm run lint && npm run test",
    "build": "babel-node tools/build.js",
    "buildAndRun": "babel-node tools/build.js && npm run open:dist",
    "testonly": "./node_modules/.bin/_mocha $npm_package_options_mocha",
    "test": "npm run lint && npm run testonly",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/charleslavon/spottr-web.git"
  },
  "keywords": [
    "spottr",
    "fitness",
    "mvp"
  ],
  "author": "Charles",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/charleslavon/spottr-web/issues"
  },
  "homepage": "https://github.com/charleslavon/spottr-web#readme",
  "dependencies": {
    "jquery": "^3.1.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "redux": "^3.6.0",
    "uuid": "^2.0.3"
  },
  "devDependencies": {
    "babel-cli": "6.10.1",
    "babel-core": "6.10.4",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.5.0",
    "babel-register": "6.9.0",
    "browser-sync": "2.13.0",
    "chai": "^3.5.0",
    "connect-history-api-fallback": "^1.3.0",
    "css-loader": "^0.25.0",
    "deep-freeze": "0.0.1",
    "enzyme": "^2.5.1",
    "eslint": "^3.8.1",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "1.5.5",
    "eslint-plugin-react": "^6.4.1",
    "eslint-watch": "^2.1.14",
    "file-loader": "^0.9.0",
    "mocha": "^3.1.2",
    "node-sass": "^3.10.1",
    "npm-run-all": "2.3.0",
    "path": "^0.12.7",
    "react-addons-test-utils": "^15.3.2",
    "sass-loader": "^4.0.2",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.0"
  }
}
